		<property name="removeAbandonedTimeout">
			<value>${mfino.dbcp.removeAbandonedTimeout}</value>
		</property>
		<property name="logAbandoned">
			<value>${mfino.dbcp.logAbandoned}</value>
		</property>
	</bean>


