<?xml version="1.0" encoding="UTF-8"?>

<logger name="Q2_thales" class="org.jpos.q2.qbean.LoggerAdaptor">
  <log-listener class="com.mfino.hsm.thales.util.ThalesLogListener">
     <property name="level" value="info" />
  </log-listener>
  <log-listener class="org.jpos.util.DailyLogListener">
    <property name="prefix" value="data/log/jpos-thales-zenith" />
    <property name="date-format" value="-yyyy-MM-dd-HH"/>
    <property name="compression-format" value="none"/> 
  </log-listener>
</logger>