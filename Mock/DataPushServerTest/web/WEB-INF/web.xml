        <servlet-class>com.mfino.datapushservertest.TestRegisterNewSubs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestUpdateSubs</servlet-name>
        <servlet-class>com.mfino.datapushservertest.TestUpdateSubs</servlet-class>
