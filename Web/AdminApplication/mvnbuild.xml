<?xml version="1.0" encoding="UTF-8"?>
<project name="AdminApplication" default="jslintall" basedir=".">
	<taskdef name="jslint" classname="com.googlecode.jslint4java.ant.JSLintTask"
		classpath="../../ThirdParty/JavaScript/jslint4java/jslint4java-1.3.1.jar" />

	<target name="jslintall">
		<jslint>
			<formatter type="plain" />
			<fileset dir="web/js" includes="**/*.js" excludes="**/ux/**" />
		</jslint>
	</target>
</project>