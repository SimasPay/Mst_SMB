<channel-adaptor name='gtbank' class="org.jpos.q2.iso.ChannelAdaptor" logger="Q2_gt">
	<channel class="org.jpos.iso.channel.PostChannel" logger="Q2_gt" packager="org.jpos.iso.packager.PostPackager">
		<property name="host" value="localhost" />
		<property name="port" value="9777" />
		<property name="packager-logger" value="Q2_gt" />
		<property name="packager-realm" value="mfino-debug" />
		<property name="packager-config" value="jpos_cfg/postpack.xml" />
	</channel>
	<in>gtsend</in>
	<out>gtreceive</out>
	<reconnect-delay>5000</reconnect-delay>
</channel-adaptor>